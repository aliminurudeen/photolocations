{
  "name": "photo-locations",
  "version": "0.2.1",
  "description": "A site for anyone seeking good photo locations",
  "repository": "https://github.com/cornelia-schulz/photo-opportunities",
  "main": "server/index.js",
  "author": "Cornelia Schulz",
  "license": "ISC",
  "scripts": {
    "build": "webpack --config ./webpack.config.js",
    "knex": "knex --knexfile ./server/db/knexfile.js",
    "start": "webpack && node server",
    "dev": "node server & webpack --watch",
    "postinstall": "yarn build && yarn knex migrate:latest",
    "test": "jest --watchAll"
  },
  "moduleFileExtensions": [
    "js",
    "json",
    "node"
  ],
  "babel": {
    "presets": [
      "@babel/env",
      "@babel/react"
    ],
    "plugins": [
      "@babel/plugin-syntax-dynamic-import",
      "@babel/plugin-syntax-import-meta",
      [
        "@babel/plugin-proposal-class-properties",
        {
          "loose": false
        }
      ],
      "@babel/plugin-proposal-json-strings"
    ]
  },
  "jest": {
    "setupTestFrameworkScriptFile": "./node_modules/jest-enzyme/lib/index.js",
    "testURL": "http://localhost:3000/"
  },
  "dependencies": {
    "@babel/core": "^7.0.0",
    "@babel/plugin-proposal-class-properties": "^7.1.0",
    "@babel/plugin-proposal-json-strings": "^7.0.0",
    "@babel/plugin-syntax-dynamic-import": "^7.0.0",
    "@babel/plugin-syntax-import-meta": "^7.0.0",
    "@babel/preset-env": "^7.0.0",
    "@babel/preset-react": "^7.0.0",
    "@babel/register": "^7.0.0",
    "babel-loader": "^8.0.4",
    "cors": "^2.8.4",
    "dotenv": "^6.0.0",
    "express": "^4.16.4",
    "google-maps": "^3.2.1",
    "google-maps-react": "^2.0.2",
    "i18next": "^11.9.0",
    "i18next-browser-languagedetector": "^2.2.3",
    "i18next-xhr-backend": "^1.5.1",
    "install": "^0.12.2",
    "jade": "^1.11.0",
    "jsonwebtoken": "^8.3.0",
    "knex": "^0.14.6",
    "nodemailer": "^4.6.8",
    "npm": "^6.4.1",
    "passport": "^0.4.0",
    "passport-facebook-token": "^3.3.0",
    "passport-google-token": "^0.1.2",
    "pg": "^7.4.3",
    "prop-types": "^15.6.2",
    "react": "^16.5.2",
    "react-dom": "^16.4.1",
    "react-facebook-login": "^4.0.1",
    "react-ga": "^2.5.3",
    "react-google-login": "^3.2.1",
    "react-i18next": "^8.0.7",
    "react-modal": "^3.5.1",
    "react-redux": "^5.0.7",
    "react-router": "^4.3.1",
    "react-router-dom": "^4.3.1",
    "react-scripts": "^2.0.5",
    "react-star-rating-component": "^1.4.1",
    "redux": "^4.0.1",
    "redux-immutable-state-invariant": "^2.1.0",
    "redux-mock-store": "^1.5.3",
    "redux-thunk": "^2.3.0",
    "request": "^2.87.0",
    "superagent": "^3.8.3",
    "webpack": "^4.20.2",
    "webpack-dev-server": "^3.1.9"
  },
  "devDependencies": {
    "@alienfast/i18next-loader": "^1.0.16",
    "@babel/plugin-syntax-object-rest-spread": "^7.0.0",
    "enzyme": "^3.7.0",
    "enzyme-adapter-react-16": "^1.6.0",
    "jest": "^23.6.0",
    "jest-enzyme": "^7.0.0",
    "sqlite3": "^4.0.2",
    "webpack-cli": "^3.1.2"
  }
}
